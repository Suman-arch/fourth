pipeline {
agent any 
options {
timestamps()
}
tools
{
jdk 'Java 8'
       maven 'Maven 3.6.3'
}
stages {
stage('Checkout')
{
steps {
git credentialsId: 'Suman Git', url: 'https://github.com/Suman-arch/fourth.git'
}
}
stage('Report')
{
steps {
script {
if(currentBuild.currentResult=='SUCCESS')
{
echo "I have done success"
}
else
{
echo "I have done failure"
}
}
}
}
}
}
