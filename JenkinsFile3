pipeline {
agent any 
options {
timestamps()
}
tools
{
jdk 'Java 8'
       maven 'Maven 3.6.3'
}
stages {
stage('Checkout')
{
steps {
git credentialsId: 'Suman Git', url: 'https://github.com/Suman-arch/fourth.git'
}
}
stage('Report')
{
options {
                timeout(time: 1, unit: 'MINUTE') 
            }
steps {
echo "${currentBuild.currentResult} is the result"
}
post {
aborted {
echo 'Aborted process'
}
success {
echo 'Success process'
}
}
}
}
}
