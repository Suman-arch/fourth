pipeline {
agent any 
options {
       timestamps()
    }
 tools {
       jdk 'Java 8'
       maven 'Maven 3.6.3'
     }
     stages {
         stage('Checkout') {
            when {
             branch 'master'
            }
             steps  {
                git credentialsId: 'Suman Git', url: 'https://github.com/Suman-arch/fourth.git'
                echo ${currentBuild.result}
                     }
                            }

}
}
